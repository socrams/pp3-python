{
    "builds":[
        {
            "src":"main.py",
            "use": "@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"main.py",
            "statusCodes":{
                "403": "/error-page/",
                "500": "/",
                "*": "/"
                },
                "headers":{
                    "Cache-Control": "s-maxage=1, stale-while-revalidate"
                    }
        }
    ]
}